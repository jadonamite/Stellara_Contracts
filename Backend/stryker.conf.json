{
  "packageManager": "npm",
  "testRunner": "jest",
  "jest": {
    "projectType": "custom",
    "configFile": "test/jest-e2e.json",
    "enableFindRelatedTests": false
  },
  "mutate": [
    "src/**/*.service.ts",
    "src/**/*.controller.ts",
    "src/**/*.util.ts",
    "src/**/*.helper.ts",
    "!src/**/*.spec.ts",
    "!src/main.ts",
    "!src/app.module.ts"
  ],
  "transpilers": [],
  "thresholds": {
    "high": 80,
    "low": 60,
    "break": 50
  },
  "reporters": [
    "html",
    "clear-text",
    "progress"
  ],
  "logLevel": "info",
  "maxConcurrentTestRunners": 2,
  "tempDirName": ".stryker-tmp",
  "cleanTempDir": true
}